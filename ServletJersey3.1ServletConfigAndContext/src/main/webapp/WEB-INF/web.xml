<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>ServletJersey3.1</display-name>

	<servlet>
		<servlet-name>AddServlet</servlet-name>
		<servlet-class>com.talowski.AddServlet</servlet-class>
		
		<init-param>
			<param-name>name</param-name>
			<param-value>Krzysztof</param-value>
		</init-param>
		
	</servlet>

	<servlet-mapping>
		<servlet-name>AddServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>SqServlet</servlet-name>
		<servlet-class>com.talowski.SqServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SqServlet</servlet-name>
		<url-pattern>/sq</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>name</param-name>
		<param-value>Kamil</param-value>
	</context-param>

	<context-param>
		<param-name>Phone</param-name>
		<param-value>Samsung</param-value>
	</context-param>



</web-app>