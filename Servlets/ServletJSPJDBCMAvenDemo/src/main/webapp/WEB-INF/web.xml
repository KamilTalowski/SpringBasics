<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetPersonController</servlet-name>
  	<display-name>GetPersonController</display-name>
  	<description></description>
  	<servlet-class>com.talowski.web.GetPersonController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddPersonController</servlet-name>
  	<display-name>AddPersonController</display-name>
  	<description></description>
  	<servlet-class>com.talowski.web.AddPersonController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetPersonController</servlet-name>
  	<url-pattern>/GetPersonController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddPersonController</servlet-name>
  	<url-pattern>/AddPersonController</url-pattern>
  </servlet-mapping>
</web-app>
